{
  "env": { "es6": true },
  "extends": ["prettier"],
  "rules": {
    "no-var": "error",
    // HtmlAcademy
    "arrow-parens": ["error", "always"],
    "prefer-rest-params": "error",
    "prefer-spread": "error",
    "quotes": ["error", "backtick"]
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  }
}
